def numpat(n): 
	
	# initialising starting number 
	num = 1

	
	for i in range(0, n): 
	
		# re assigning num 
		num = 1
	
		
		for j in range(0, i+1): 
		
				# printing number 
			print(num, end=" ") 
		
			# incrementing number at each column 
			num = num + 1
	
		# ending line after each row 
		print("\r") 

# Driver code 
n = 5
numpat(n) 
